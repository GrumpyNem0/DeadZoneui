function dzchat(String)
	game.StarterGui:SetCore("ChatMakeSystemMessage", {
		Text = '[DEADZONE MULTITOOL]: ' .. String
	})
end
local number = math.random(1,10)
dzchat("Loading . . .")
wait(1)
dzchat("Done!")
dzchat("Have fun!")
if number == 7 then
	dzchat("Credits: Grumpy Nemo#5103 // btw love u Xuicl & TV/ChillBro")
else
	dzchat("Credits: Grumpy Nemo#5103")
end
