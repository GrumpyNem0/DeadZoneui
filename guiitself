local function XSNM_fake_script() -- Speed.LocalScript 
	local script = Instance.new('LocalScript', Speed)

	script.parent.MouseButton1Click:Connect(function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/GrumpyNem0/DeadZoneui/main/SpeedScript'))()
	end)
end
coroutine.wrap(XSNM_fake_script)()
local function CESO_fake_script() -- PageHolder.glb 
	local script = Instance.new('LocalScript', PageHolder)

	script.Parent.Self.Visible = false
	script.Parent.Random.Visible = false
	--
	_G.DupeDMT = false
	while _G.DupeDMT==true do
		wait(0.1)
		game.ReplicatedStorage.RemoteFunctions.MoveItem:InvokeServer(47,48)
		game.ReplicatedStorage.RemoteFunctions.MoveItem:InvokeServer(48,47)
	end
	--
	_G.Self = script.Parent.Self
	_G.Rando = script.Parent.Random
	_G.customlegbroketextDMT="haha u broke leg nerd (leg broken use a splint to fix.) -Nemo."
end
coroutine.wrap(CESO_fake_script)()
local function PXFAM_fake_script() -- Self_2.LocalScript 
	local script = Instance.new('LocalScript', Self_2)

	script.parent.MouseButton1Click:Connect(function()
		_G.MultiToolTitle.Text = "DEADZONE FUCKER // SELF"
		_G.Self.Visible = true
		_G.Rando.Visible = false
	end)
end
coroutine.wrap(PXFAM_fake_script)()
