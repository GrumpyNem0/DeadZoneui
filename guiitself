-- Scripts:

local function SDLLX_fake_script() -- Title.LocalScript 
	local script = Instance.new('LocalScript', Title)

	_G.MultiToolTitle = script.Parent
end
coroutine.wrap(SDLLX_fake_script)()
local function RZZIJMB_fake_script() -- SpeedButton.BttnScript 
	local script = Instance.new('LocalScript', SpeedButton)

	_G.PlayerSpeedDMT = "22"
	script.parent.MouseButton1Click:Connect(function()
	_G.PlayerSpeedDMT = script.Parent.Parent.SpeedBox.Text
	end)
end
coroutine.wrap(RZZIJMB_fake_script)()
local function PUXIQGA_fake_script() -- JumpButton.BttnScript 
	local script = Instance.new('LocalScript', JumpButton)

	_G.JumpPowerDMT = "26"
	script.parent.MouseButton1Click:Connect(function()
	_G.JumpPowerDMT = script.Parent.Parent.JumpBox.Text
	end)
end
coroutine.wrap(PUXIQGA_fake_script)()
