if game.Players.LocalPlayer.PlayerGui:FindFirstChild("Goggles") then
    game.Players.LocalPlayer.PlayerGui:FindFirstChild("Goggles"):Destroy()
end
if game.Lighting:FindFirstChild("Fog") then
game.Lighting:FindFirstChild("Fog"):Destroy()
end
if game.Lighting:FindFirstChild("ambient") then
game.Lighting:FindFirstChild("ambient"):Destroy()
end
while true do
game.Lighting.FogStart = _G.DMTFogStart
game.Lighting.Brightness = _G.DMTBrightness
wait()
end
