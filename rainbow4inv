	local Slotone = game.Players.LocalPlayer.PlayerGui.BackpackGui.Frame["1"]
	local Slottwo = game.Players.LocalPlayer.PlayerGui.BackpackGui.Frame["2"]
	local Slotthree = game.Players.LocalPlayer.PlayerGui.BackpackGui.Frame["3"]
	local Slotfour = game.Players.LocalPlayer.PlayerGui.BackpackGui.Frame["4"]
	----
	--1
	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		Slotone.ImageColor3 = Color3.fromHSV(zigzag(counter),1,1)
		Slottwo.ImageColor3 = Color3.fromHSV(zigzag(counter),1,1)
		Slotthree.ImageColor3 = Color3.fromHSV(zigzag(counter),1,1)
		Slotfour.ImageColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.01
end
	----
